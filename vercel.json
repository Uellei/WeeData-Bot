{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"
        }
      },
      {
        "src": "frontend/src/api/proxy.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/proxy",
        "dest": "/frontend/src/api/proxy.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/$1"
      }
    ]
  }
  